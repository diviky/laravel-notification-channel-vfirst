{
    "name": "laravel-notification-channels/vfirst",
    "description": "Value First Notifications driver for sms",
    "homepage": "https://github.com/laravel-notification-channels/vfirst",
    "license": "MIT",
    "require": {
        "illuminate/notifications": "^5|^6|^7",
        "illuminate/support": "^5|^6|^7",
        "illuminate/events": "^5|^6|^7"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Vfirst\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Vfirst\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Vfirst\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}